type
  TPepsi = class(TInterfacedObject, ISoda)

  end;

  TWhopper = class(TInterfacedObject, ISoda)

  end;

  TBurgerKing = class(TInterfacedObject, IFactoryFood)
    function GetHamburger: IHamburger;
    function GetSoda: ISoda;
  end;


implementation

end.
